{
	"name": "schulcloud-nuxt",
	"version": "1.0.0",
	"description": "The next Level of Schul-Cloud",
	"author": "HPI Schul-Cloud",
	"private": true,
	"scripts": {
		"dev": "cross-env NODE_ENV=development nodemon server/index.js --watch server",
		"build": "nuxt build",
		"start": "nuxt start",
		"generate": "nuxt generate",
		"lint:eslint": "eslint --fix",
		"lint:stylelint": "stylelint --fix",
		"lint:markdownlint": "markdownlint",
		"lint:prettier": "prettier --write --loglevel warn",
		"lint:all:eslint": "yarn lint:eslint --ext .js,.vue .",
		"lint:all:stylelint": "yarn lint:stylelint \"**/*.{vue,sass,scss,css}\"",
		"lint:all:markdownlint": "yarn lint:markdownlint \"*/*.md\" \"*.md\"",
		"lint:all:prettier": "yarn lint:prettier \"**/*.{js,json,css,scss,vue,html,md}\"",
		"lint": "run-s lint:all:*"
	},
	"gitHooks": {
		"pre-commit": "cross-env PRE_COMMIT=true lint-staged"
	},
	"dependencies": {
		"@feathersjs/authentication-client": "^1.0.8",
		"@feathersjs/feathers": "^3.2.3",
		"@feathersjs/socketio-client": "^1.1.5",
		"@fortawesome/fontawesome-svg-core": "^1.2.13",
		"@fortawesome/free-solid-svg-icons": "^5.7.0",
		"@fortawesome/vue-fontawesome": "^0.1.5",
		"@nuxtjs/axios": "^5.0.0",
		"cookie-storage": "^3.2.0",
		"feathers-vuex": "^1.6.2",
		"handlebars": "^4.0.12",
		"handlebars-layouts": "^3.1.4",
		"handlebars-wax": "^6.1.0",
		"lodash": "^4.17.11",
		"moment": "^2.23.0",
		"nuxt": "^2.0.0",
		"nuxt-buefy": "^0.2.1",
		"nuxt-express-module": "0.0.11",
		"socket.io-client": "^2.2.0"
	},
	"devDependencies": {
		"@vue/cli-plugin-babel": "^3.3.0",
		"@vue/cli-plugin-e2e-cypress": "^3.3.0",
		"@vue/cli-plugin-eslint": "^3.3.0",
		"@vue/cli-plugin-unit-jest": "^3.3.0",
		"@vue/cli-service": "^3.3.1",
		"@vue/eslint-config-prettier": "^4.0.1",
		"@vue/eslint-config-standard": "^4.0.0",
		"@vue/test-utils": "^1.0.0-beta.28",
		"babel-core": "^6.26.3",
		"babel-eslint": "^8.2.1",
		"babel-jest": "^24.0.0",
		"cross-env": "^5.2.0",
		"eslint": "^5.0.1",
		"eslint-config-prettier": "^3.1.0",
		"eslint-loader": "^2.0.0",
		"eslint-plugin-prettier": "2.6.2",
		"eslint-plugin-vue": "^4.7.1",
		"imagemin-lint-staged": "^0.4.0",
		"lint-staged": "^8.1.1",
		"markdownlint-cli": "^0.13.0",
		"node-sass": "^4.11.0",
		"nodemon": "^1.11.0",
		"npm-run-all": "^4.1.5",
		"nuxt-babel": "^0.1.2",
		"prettier": "github:prettier/prettier#3de36e3",
		"sass-loader": "^7.1.0",
		"stylelint": "^9.10.1",
		"stylelint-config-css-modules": "^1.3.0",
		"stylelint-config-prettier": "^4.0.0",
		"stylelint-config-recess-order": "^2.0.1",
		"stylelint-config-standard": "^18.2.0",
		"stylelint-scss": "^3.5.1"
	}
}
